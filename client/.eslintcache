      return \"word-bold hidden\";\r\n        } else {\r\n          return \"word-bold\";\r\n        }\r\n      } else {\r\n        return \"word-bold\";\r\n      }\r\n    } else {\r\n      if (index > counter) {\r\n        if (!stop) {\r\n          return \"regular hidden\";\r\n        } else {\r\n          return \"regular\";\r\n        }\r\n      } else {\r\n        return \"regular\";\r\n      }\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <button\r\n        className={nameOfClass}\r\n        onClick={() => playAgain()}\r\n        onMouseDown={() => MouseDown()}\r\n        onMouseUp={() => MouseUp()}\r\n      >\r\n        <LoopIcon style={{ fontSize: \"40px\", color: \"white\" }} />\r\n      </button>\r\n      {audio && (\r\n        <div>\r\n          {audio.map((obj, i) => (\r\n            <span className={getClassName(obj.itsWord, i)}>{obj.text} </span>\r\n          ))}\r\n        </div>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\App.js",[],"C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\ts\\AudioVisualizer.js",[],"C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\ts\\AudioAnalyzer.js",["88"],"C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\ts\\index.js",[],"C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\components\\dashboard\\Selector.js",[],"C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\components\\dashboard\\Dashboard.js",["89"],"import React, { useState, useEffect } from \"react\";\r\nimport Selector from \"./Selector\";\r\nimport { sortArray, filterArray } from \"./helpersFunctions\";\r\nimport axios from \"axios\";\r\n\r\nexport default function Dashboard() {\r\n  const [beDisabled, SetBeDisabled] = useState(true);\r\n  const [languagesArr, setLanguagesArr] = useState([]);\r\n  const [nativeLanguage, setNativeLanguage] = useState();\r\n  const [learn