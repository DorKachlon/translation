       setTimeout(() => {\r\n          setServerAudio(data.audio);\r\n        }, 2000);\r\n      }\r\n    })();\r\n  }, [clientAudio]);\r\n\r\n  const getAnExercise = async () => {\r\n    const { data } = await network.post(\"/api/v1/exercise\");\r\n    setServerAudio(data.audio);\r\n    setSuccess(false);\r\n    setSaidWord(\"\");\r\n  };\r\n\r\n  useEffect(() => {\r\n    (async () => {\r\n      if (serverAudio) {\r\n        if (counter <= serverAudio.length) {\r\n          playAudio();\r\n        }\r\n      }\r\n    })();\r\n  }, [counter]);\r\n\r\n  useEffect(() => {\r\n    (async () => {\r\n      if (serverAudio) {\r\n        setCounter(0);\r\n      }\r\n    })();\r\n  }, [serverAudio]);\r\n\r\n  const playAudio = () => {\r\n    if (counter < serverAudio.length) {\r\n      const audioToPlay = new Audio(`data:audio/ogg;base64, ${serverAudio[counter].base64}`);\r\n      audioToPlay.play();\r\n      audioToPlay.onended = () => {\r\n        setCounter((prev) => prev + 1);\r\n      };\r\n    } else {\r\n      debugger;\r\n      if (success) {\r\n        getAnExercise();\r\n      } else {\r\n        startRecording();\r\n      }\r\n    }\r\n  };\r\n\r\n  const getClassName = (itsWord, index) => {\r\n    if (itsWord) {\r\n      if (index > counter) {\r\n        return \"word-bold hidden\";\r\n      } else {\r\n        return \"word-bold\";\r\n      }\r\n    } else {\r\n      if (index > counter) {\r\n        return \"regular hidden\";\r\n      } else {\r\n        return \"regular\";\r\n      }\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <PlayAgainButton playAudio={playAudio} />\r\n      {serverAudio && (\r\n        <div className=\"server-text\">\r\n          {serverAudio.map((obj, i) => (\r\n            <span className={getClassName(obj.itsWord, i)}>{obj.text} </span>\r\n          ))}\r\n        </div>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\dor1k\\Desktop\\Cyber4s\\01 GITHUB\\translation\\client\\src\\App.js",[],"C:\\Users\\dor1k\\Desktop\\Cyber